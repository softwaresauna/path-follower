{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "typedef": [
            true,
            "call-signature",
            "property-declaration"
        ],
        "quotemark": false,
        "interface-name": false,
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "trailing-comma": false,
        "member-access": [true, "no-public"],
        "typedef": {
            "options": "call-signature"
        },
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}
